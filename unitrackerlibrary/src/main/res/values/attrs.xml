<resources>
    <declare-styleable name="ExpandableTextView">
        <attr format="string" name="title"/>
        <attr format="string" name="text"/>
    </declare-styleable>
    <declare-styleable name="LabelledSeekBar">
        <attr format="boolean" name="editableLabel"/>
        <attr format="integer" name="max"/>
        <attr format="float" name="current"/>
    </declare-styleable>
    <declare-styleable name="SplitPaneLayout">
        <attr name="orientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr format="dimension" name="splitterSize"/>
        <attr format="boolean" name="splitterMovable"/>
        <attr format="dimension|fraction" name="splitterPosition"/>
        <attr format="reference|color" name="splitterBackground"/>
        <attr format="reference|color" name="splitterDraggingBackground"/>
        <attr format="dimension" name="splitterTouchSlop"/>
        <attr format="dimension" name="paneSizeMin"/>
    </declare-styleable>
    <declare-styleable name="SwipeRefreshDeleteList">
        <attr format="integer" name="itemIcon"/>
        <attr format="integer" name="listItemBackground"/>
        <attr format="integer" name="listItemBackgroundStatePositive"/>
        <attr format="color" name="listItemForeground"/>
        <attr format="integer" name="selectedListItemBackground"/>
        <attr format="boolean" name="readOnly"/>
        <attr format="integer" name="listItemDivider"/>
        <attr format="color" name="iconColor"/>
    </declare-styleable>
    <declare-styleable name="WidgetCalendar">
        <attr format="integer" name="toolBarBackground"/>
        <attr format="integer" name="monthViewBackground"/>
        <attr format="integer" name="dayViewBackground"/>
        <attr format="integer" name="focusBackground"/>
        <attr format="dimension" name="groupLabelWidth"/>
    </declare-styleable>

    <declare-styleable name="DropDown">
        <attr name="dropdown_title" format="string"/>
    </declare-styleable>
</resources>